{
  "name": "@ar.io/sdk",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ar-io/ar-io-sdk.git"
  },
  "main": "./lib/cjs/node/index.js",
  "module": "./lib/esm/node/index.js",
  "types": "./lib/types/node/index.d.ts",
  "browser": "./bundles/web.bundle.min.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "license": "AGPL-3.0-or-later",
  "files": [
    "lib",
    "bundles",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Permanent Data Solutions Inc",
    "email": "info@ar.io",
    "website": "https://ar.io"
  },
  "keywords": [
    "arweave",
    "ar",
    "blockchain",
    "warp",
    "contracts",
    "ar.io",
    "ao"
  ],
  "exports": {
    ".": {
      "import": "./lib/esm/node/index.js",
      "require": "./lib/cjs/node/index.js",
      "types": "./lib/types/node/index.d.ts",
      "browser": "./bundles/web.bundle.min.js"
    },
    "./node": {
      "import": "./lib/esm/node/index.js",
      "require": "./lib/cjs/node/index.js",
      "types": "./lib/types/node/index.d.ts"
    },
    "./web": {
      "import": "./bundles/web.bundle.min.js",
      "require": "./bundles/web.bundle.min.js",
      "types": "./lib/types/web/index.d.ts",
      "browser": "./bundles/web.bundle.min.js"
    }
  },
  "scripts": {
    "build:web": "node bundle.mjs",
    "build:esm": "yarn tsc -p tsconfig.json",
    "build:cjs": "yarn tsc -p tsconfig.cjs.json && echo \"{\\\"type\\\": \\\"commonjs\\\"}\" > lib/cjs/package.json",
    "build": "yarn clean && yarn build:web && yarn build:esm && yarn build:cjs",
    "clean": "rimraf [ lib coverage bundles tests/contracts tests/wallets ]",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "yarn clean && jest --config=jest.config.mjs",
    "test:integration": "yarn clean && docker compose up -d && jest --config=jest.integration.config.mjs --runInBand && docker compose down -v",
    "prepare": "husky install",
    "example:mjs": "yarn build:esm && node examples/node/index.mjs",
    "example:cjs": "yarn build:cjs && node examples/node/index.cjs",
    "example:web": "yarn build:web && http-server --port 8080 --host -o examples/web"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.4.8",
    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^6.4.0",
    "esbuild": "^0.19.2",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.7",
    "sinon": "^15.2.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "warp-contracts-plugin-deploy": "^1.0.13"
  },
  "dependencies": {
    "arbundles": "0.11.0",
    "arweave": "1.14.4",
    "axios": "1.4.0",
    "setimmediate": "^1.0.5",
    "warp-arbundles": "^1.0.4",
    "warp-contracts": "1.4.39",
    "winston": "^3.11.0"
  },
  "lint-staged": {
    "**/*.{ts,js,mjs,cjs,md,json}": [
      "prettier --write ."
    ]
  }
}
